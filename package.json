{
  "name": "pokemon-query",
  "version": "1.0.0",
  "description": "pokemon-API",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node ./dist/server.js",
    "prod": "npm run clean && webpack && babel server --out-dir dist",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "prod:compress": "npm run prod -- --env.presets compress",
    "dev": "webpack-dev-server --env.mode development --open",
    "dev-server": "nodemon --exec babel-node ./server/server.js",
    "format": "prettier --write \"src/**/*.{js,jsx,html}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js --env.mode production",
    "test": "jest",
    "test-routes": "npm run test -t router",
    "test-model": "npm run test -t model",
    "test-controller": "npm run test -t controller"
  },
  "keywords": [
    "react",
    "express.js",
    "MongoDb"
  ],
  "author": "RL",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.0",
    "@babel/core": "^7.10.0",
    "@babel/node": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-react": "^7.10.0",
    "@testing-library/react": "^9.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "serialize-javascript": "^3.1.0",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.9.16",
    "node-fetch": "^2.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Firefox versions",
    "last 2 FirefoxAndroid versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions"
  ]
}
